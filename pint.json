{
    "preset": "laravel",
    "rules": {
        "method_chaining_indentation": true,
        "concat_space": {
            "spacing": "one"
        },
        "explicit_string_variable": true,
        "no_useless_else": true,
        "no_useless_return": true,
        "ordered_class_elements": {
            "order": [
                "use_trait",
                "case",
                "constant_public",
                "constant_protected",
                "constant_private",
                "property_public",
                "property_protected",
                "property_private",
                "construct",
                "destruct",
                "method_static",
                "method_public",
                "method_protected",
                "magic",
                "method_scope",
                "phpunit",
                "method_is",
                "method_private"
            ],
            "sort_algorithm": "none"
        }
    },
    "finder": {
        "exclude": [],
        "in": [
            "app",
            "app/Models"
        ]
    }
}